<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quiplash">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".FriendsActivity" />
        <activity android:name=".Profile_RegisteredActivity" />
        <activity android:name=".Join_GameActivity" />
        <activity android:name=".New_GameActivity" />
        <activity android:name=".LandingActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".Database" />
        <activity android:name=".Host_WaitingActivity" />
        <activity android:name=".GameLaunchingActivity" />
        <activity android:name=".GlobalScoreboard_Activity" />
        <activity android:name=".AddQuestion" />
        <activity android:name=".ChooseQuestionTypeActivity"></activity>
        <activity android:name=".AnswersActivity" />
        <activity android:name=".PrepareAnswerActivity" />
        <activity android:name=".Choose_AnswerActivity" />
        <activity android:name=".EvaluationActivity" />
        <activity android:name=".End_Of_GameActivity" />
        <activity android:name=".Edit_ProfileActivity" />
        <activity android:name=".Edit_PW_Mail_Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>