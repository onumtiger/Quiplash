<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quiplash">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ResetPasswordActivity"/>
        <activity android:name=".FriendsActivity" />
        <activity android:name=".Profile_RegisteredActivity" />
        <activity android:name=".Join_GameActivity" />
        <activity android:name=".New_GameActivity" />
        <activity android:name=".LandingActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".Database" />
        <activity android:name=".Host_WaitingActivity" />
        <activity android:name=".Game_LaunchingActivity" />
        <activity android:name=".QuestionActivity" />
        <activity android:name=".Prepare_AnswerActivity" />
        <activity android:name=".Choose_AnswerActivity" />
        <activity android:name=".EvaluationActivity" />
        <activity android:name=".End_Of_GameActivity" />
        <activity android:name=".Edit_ProfileActivity"/>
        <activity android:name=".Edit_PW_Mail_Activity"/>
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest>